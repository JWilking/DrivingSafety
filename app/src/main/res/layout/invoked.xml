<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/RelativeLayout02" 
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:layout_gravity="center_vertical|center_horizontal"
 		android:layout_height="wrap_content"
 		android:layout_width="wrap_content">

  <ImageView android:id="@+id/InvokedActivityImage"
 		android:src="@drawable/drunk"
 		android:layout_width="30dp"
 		android:layout_height="30dp">
 </ImageView>
 
  <TextView android:id="@+id/InvokedActivityText"
 		android:text="@string/NotYet"
 		android:layout_below="@+id/InvokedActivityImage"
 		android:layout_gravity="center_vertical|center_horizontal"
 		android:layout_width="40dp"
 		android:layout_height="40dp">
 </TextView>
 
   <Button android:id="@+id/OK"
   		  android:layout_below="@+id/InvokedActivityText"
   		  android:layout_gravity="center_vertical|center_horizontal"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="Return"/>  
           
</RelativeLayout>
